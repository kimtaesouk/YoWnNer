<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 둥근 모양의 외곽선을 그립니다 -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#000000" /> <!-- 내부를 투명으로 설정 -->
            <stroke
                android:width="3dp"
                android:color="#00EAE9E9" /> <!-- 선의 두께와 색상을 설정 -->
            <corners android:radius="30dp" /> <!-- 둥근 모양 설정 -->
        </shape>
    </item>
</layer-list>

